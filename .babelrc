{
    "presets": [
      "@babel/env",
      "@babel/react",
      "@babel/typescript"
    ],
    "plugins": [
      "@babel/proposal-optional-chaining",
      "@babel/proposal-object-rest-spread",
      ["@babel/proposal-decorators", { "legacy": true }],
      ["@babel/plugin-proposal-class-properties", { "loose" : true }]
    ]
}